{
  "name": "@health.gov.au/health-design-system",
  "author": "Australian Government Department of Health <enquiries@health.gov.au> (https://www.health.gov.au)",
  "description": "Design system for health.gov.au and related products.",
  "version": "1.4.0-alpha.17",
  "repository": {
    "type": "git",
    "url": "https://github.com/healthgovau/health-design-system.git"
  },
  "license": "GNU GENERAL PUBLIC LICENSE v3",
  "bugs": {
    "url": "https://github.com/healthgovau/health-design-system/issues"
  },
  "main": "index.js",
  "bin": {
    "healthds": "./bin/cli.js",
    "hdssetup": "bin/setup.js"
  },
  "dependencies": {
    "@gold.au/accordion": "*",
    "@gold.au/body": "*",
    "@gold.au/breadcrumbs": "*",
    "@gold.au/buttons": "*",
    "@gold.au/callout": "*",
    "@gold.au/card": "*",
    "@gold.au/control-input": "*",
    "@gold.au/core": "*",
    "@gold.au/cta-link": "*",
    "@gold.au/direction-links": "*",
    "@gold.au/footer": "*",
    "@gold.au/grid-12": "*",
    "@gold.au/header": "*",
    "@gold.au/headings": "*",
    "@gold.au/inpage-nav": "*",
    "@gold.au/keyword-list": "*",
    "@gold.au/link-list": "*",
    "@gold.au/main-nav": "*",
    "@gold.au/page-alerts": "*",
    "@gold.au/pancake-js": "*",
    "@gold.au/responsive-media": "*",
    "@gold.au/select": "*",
    "@gold.au/side-nav": "*",
    "@gold.au/skip-link": "*",
    "@gold.au/tags": "*",
    "@gold.au/text-inputs": "*",
    "copy": "^0.3.2",
    "fs-extra": "^10.0.0",
    "get-dependencies": "^1.1.0",
    "get-installed-path": "^4.0.8"
  },
  "devDependencies": {
    "@frctl/fractal": "^1.2.1",
    "@frctl/mandelbrot": "^1.2.1",
    "backstopjs": "^5.3.4",
    "del": "^5.0.0",
    "gulp": "^4.0.2",
    "gulp-autoprefixer": "^6.1.0",
    "gulp-clean-css": "^4.2.0",
    "gulp-concat": "^2.6.1",
    "gulp-copy": "^4.0.1",
    "gulp-debug": "^4.0.0",
    "gulp-header": "^2.0.9",
    "gulp-minify": "^3.1.0",
    "gulp-rename": "^1.4.0",
    "gulp-sass": "^4.0.2",
    "mandelbrot-healthgovau": "git+https://github.com/healthgovau/mandelbrot-healthgovau.git#1.0.0",
    "sass-versioning": "^0.4.0"
  },
  "engines": {
    "node": ">=4.0.0"
  },
  "scripts": {
    "start": "cat .githooks/pre-commit > .git/hooks/pre-commit && chmod a+x .git/hooks/pre-commit && npm rebuild node-sass",
    "backstop:accept": "backstop approve --config=backstop.config.js",
    "backstop:approve": "backstop approve --config=backstop.config.js",
    "backstop:open": "backstop openReport --config=backstop.config.js",
    "backstop:run": "sh scripts/visual_regression_tests.sh",
    "gulp": "gulp",
    "gulp:watch": "gulp watch",
    "fractal:start": "cd style-guide && fractal start --sync",
    "fractal:build": "npm run gulp && cd style-guide && fractal build"
  },
  "pancake": {
    "auto-save": false,
    "plugins": true,
    "ignore": [],
    "css": {
      "name": false
    },
    "sass": {
      "modules": false,
      "location": "./source/sass/vendors/",
      "name": "govau-ds.scss"
    },
    "js": {
      "minified": true,
      "modules": false,
      "location": "./source/js/src",
      "name": "govau-ds.js"
    },
    "react": {
      "location": false
    },
    "json": {
      "enable": false
    }
  }
}
